{
    "name": "@bojavou/acorn-import-attributes",
    "description": "Import attributes for acorn",
    "version": "1.0.1",
    "homepage": "https://github.com/bojavou/acorn-import-attributes",
    "bugs": "https://github.com/bojavou/acorn-import-attributes/issues",
    "license": "MIT",
    "repository": "github:bojavou/acorn-import-attributes",
    "keywords": ["import-attributes", "import-assertions", "acorn"],
    "type": "module",
    "exports": "./package.mjs",
    "directories": {
        "lib": "lib",
        "test": "test"
    },
    "imports": {
        "#lib/*": "./lib/*",
        "#test/*": "./test/*"
    },
    "scripts": {
        "lint": "run-s -sc lint:*",
        "lint:js": "eslint --ext cjs,js,mjs .",
        "test": "ava test"
    },
    "devDependencies": {
        "ava": "^5.3.1",
        "eslint": "^8.45.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-n": "^16.0.1",
        "eslint-plugin-promise": "^6.1.1",
        "npm-run-all": "^4.1.5"
    },
    "dependencies": {
        "acorn": "^8.10.0"
    }
}
